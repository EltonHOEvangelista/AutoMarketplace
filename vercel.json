{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "auto-mp-client/dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/auto-mp-client/dist/$1"
      }
    ]
  }