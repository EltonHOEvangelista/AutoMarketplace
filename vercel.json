{
  "version": 2,
  "builds": [
    {
      "src": "auto-mp-client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "auto-mp-client/dist"
      }
    },
    {
      "src": "server/package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "server/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/auto-mp-client/dist/$1"
    }
  ]
}


  